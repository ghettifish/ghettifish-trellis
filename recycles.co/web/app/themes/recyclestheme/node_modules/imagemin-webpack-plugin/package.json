  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-plugin-transform-object-rest-spread": "6.19.0",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-env": "0.0.8",
    "gulp": "3.9.1",
    "gulp-npm-script-sync": "1.1.0",
    "gulp-run-command": "0.0.7",
    "np": "2.10.1",
    "rimraf": "2.5.4",
    "standard": "8.5.0"
